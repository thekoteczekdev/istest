{
	"rules": {
		".write":"auth.uid == 'OWNER_ID'",
		".read": "auth.uid == 'OWNER_ID'",
		"shops":{
			"$shop_id":{
				".read":true
			}
		},
		"servers":{
			"$server_id":{
				"serverName":{
					".read":true
				},
				"commands":{
					"$key":{
						".read":true,
						"$command_id":{
							".write":"!newData.exists()"
						}
					}
				}
			}
		},
		"config":{
			"$shop_id":{
				"microsms_sms_text":{
					".read":true
				},
				"lvlup":{
					".read":true
				},
				"microsms":{
					".read":true
				},
				"paypal":{
					".read":true
				},
			}
		}
	}
}
